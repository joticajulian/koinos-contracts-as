{
  "name": "@koinosbox/contracts",
  "version": "1.0.8",
  "license": "MIT",
  "dependencies": {
    "koilib": "^5.5.4",
    "koinos-precompiler-as": "^0.4.1"
  },
  "main": "assembly/index.ts",
  "files": [
    "assembly"
  ],
  "devDependencies": {
    "@as-covers/core": "^0.2.1",
    "@as-pect/cli": "^6.2.4",
    "@koinos/mock-vm": "1.0.0",
    "@koinos/sdk-as": "1.0.0",
    "@koinosbox/contracts": "^1.0.8",
    "@roaminroe/protoc": "^2.4.0",
    "@tsconfig/node16": "^1.0.4",
    "@types/node": "^16.18.3",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "as-proto": "npm:@koinos/as-proto@1.0.0",
    "assemblyscript": "^0.19.22",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.25.0",
    "fs-extra": "^11.1.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.8.4",
    "yaml": "^2.1.3"
  },
  "scripts": {
    "hello:asbuild:debug": "asc contracts/hello/build/index.ts --config contracts/hello/asconfig.json --use abort= --target debug",
    "hello:asbuild:release": "asc contracts/hello/build/index.ts --config contracts/hello/asconfig.json --use abort= --target release",
    "hello:build": "yarn hello:precompile && yarn hello:asbuild:release",
    "hello:precompile": "koinos-precompiler-as contracts/hello",
    "hello:test": "asp --verbose --config contracts/hello/as-pect.config.js",
    "hello:deploy": "ts-node contracts/hello/scripts/deployment.ts",
    "hello:info": "node getSha256.js hello",
    "token:asbuild:debug": "asc contracts/token/build/index.ts --config contracts/token/asconfig.json --use abort= --target debug",
    "token:asbuild:release": "asc contracts/token/build/index.ts --config contracts/token/asconfig.json --use abort= --target release",
    "token:build": "yarn token:precompile && yarn token:asbuild:release",
    "token:precompile": "koinos-precompiler-as contracts/token",
    "token:test": "asp --verbose --config contracts/token/as-pect.config.js",
    "token:deploy": "ts-node contracts/token/scripts/deployment.ts",
    "token:info": "node getSha256.js token",
    "token:docker": "docker build --no-cache --progress=plain --build-arg CONTRACT=token -t temp-image . && docker rmi temp-image",
    "manasharer:asbuild": "asc contracts/manasharer/build/index.ts --config contracts/manasharer/asconfig.json --use abort= --target release",
    "manasharer:precompile": "koinos-precompiler-as contracts/manasharer",
    "manasharer:build": "yarn manasharer:precompile && yarn manasharer:asbuild",
    "manasharer:initialdeploy": "ts-node contracts/manasharer/scripts/initialDeploy.ts",
    "freemanasharer:asbuild:debug": "asc contracts/freemanasharer/build/index.ts --config contracts/freemanasharer/asconfig.json --use abort= --target debug",
    "freemanasharer:asbuild:release": "asc contracts/freemanasharer/build/index.ts --config contracts/freemanasharer/asconfig.json --use abort= --target release",
    "freemanasharer:build": "yarn freemanasharer:precompile && yarn freemanasharer:asbuild:release",
    "freemanasharer:precompile": "koinos-precompiler-as contracts/freemanasharer",
    "freemanasharer:test": "asp --verbose --config contracts/freemanasharer/as-pect.config.js",
    "freemanasharer:deploy": "ts-node contracts/freemanasharer/scripts/deployment.ts",
    "freemanasharer:info": "node getSha256.js freemanasharer",
    "freemanasharer:docker": "docker build --no-cache --progress=plain --build-arg CONTRACT=freemanasharer -t temp-image . && docker rmi temp-image",
    "burnkoinhelper:asbuild:debug": "asc contracts/burnkoinhelper/build/index.ts --config contracts/burnkoinhelper/asconfig.json --use abort= --target debug",
    "burnkoinhelper:asbuild:release": "asc contracts/burnkoinhelper/build/index.ts --config contracts/burnkoinhelper/asconfig.json --use abort= --target release",
    "burnkoinhelper:build": "yarn burnkoinhelper:precompile && yarn burnkoinhelper:asbuild:release",
    "burnkoinhelper:precompile": "koinos-precompiler-as contracts/burnkoinhelper",
    "burnkoinhelper:test": "asp --verbose --config contracts/burnkoinhelper/as-pect.config.js",
    "burnkoinhelper:deploy": "ts-node contracts/burnkoinhelper/scripts/deployment.ts",
    "burnkoinhelper:info": "node getSha256.js burnkoinhelper",
    "burnkoinhelper:docker": "docker build --no-cache --progress=plain --build-arg CONTRACT=burnkoinhelper -t temp-image . && docker rmi temp-image",
    "textparserlib:asbuild:debug": "asc contracts/textparserlib/build/index.ts --config contracts/textparserlib/asconfig.json --use abort= --target debug",
    "textparserlib:asbuild:release": "asc contracts/textparserlib/build/index.ts --config contracts/textparserlib/asconfig.json --use abort= --target release",
    "textparserlib:build": "yarn textparserlib:precompile && yarn textparserlib:asbuild:release",
    "textparserlib:precompile": "koinos-precompiler-as contracts/textparserlib",
    "textparserlib:test": "asp --verbose --config contracts/textparserlib/as-pect.config.js",
    "textparserlib:deploy": "ts-node contracts/textparserlib/scripts/deployment.ts",
    "textparserlib:info": "node getSha256.js textparserlib",
    "textparserlib:docker": "docker build --no-cache --progress=plain --build-arg CONTRACT=textparserlib -t temp-image . && docker rmi temp-image",
    "release": "node updateRelease.js",
    "lint": "eslint --max-warnings 0 --ext ts \"*/assembly/**/*.ts\""
  },
  "config": {
    "protocVersion": "21.8"
  }
}
